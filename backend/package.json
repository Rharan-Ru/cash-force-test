{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node src/server.js",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "docker-migrate": "docker exec -it cashforcetest_backend_1 npx sequelize db:migrate",
    "docker-seed": "docker exec -it cashforcetest_backend_1 npx sequelize db:seed:all",
    "docker-undo-migrate": "docker exec -it cashforcetest_backend_1 npx sequelize db:migrate:undo:all",
    "docker-undo-seed": "docker exec -it cashforcetest_backend_1 npx sequelize db:seed:undo:all"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
